{
  "query": {
    "bool": {
      "must": [
        {
          "bool" : {
            "must" : [
              {
                "term" : {
                  "packageNm" : {
                    "value" : #{packageNm}
                  }
                }
              },
              {
                "term" : {
                  "serverType" : {
                    "value" : #{serverType}
                  }
                }
              },
              {
                "term" : {
                  "activeSt" : {
                    "value" : "Y"
                  }
                }
              },
              {
                "bool": {
                  "must_not" : [
                    {
                      "term": {
                        "reinstalledDate": {
                          "value" : "0"
                        }
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  }
}