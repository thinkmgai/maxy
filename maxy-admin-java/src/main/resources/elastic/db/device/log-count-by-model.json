{
  "aggs": {
    "count": {
      "terms": {
        "field": "deviceModel",
        "size": 9999,
        "order": {
          "_count": "desc"
        }
      },
      "aggs": {
        "errorCnt": {
          "sum": {
            "field": "errorCount"
          }
        },
        "crashCnt": {
          "sum": {
            "field": "crashCount"
          }
        }
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "pageStartTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  }
}