{
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        {
          "range": {
            "logTm": {
              "from": #{from},
              "to": #{to}
            }
          }
        }
      ]
    }
  }
}