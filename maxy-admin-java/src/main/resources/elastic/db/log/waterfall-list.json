{
  "size": 10000,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "logTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        },
        {
          "term": {
            "deviceId.raw": #{deviceId}
          }
        }
      ]
    }
  },
  "sort": [
    {
      "logTm": {
        "order": "asc"
      }
    }
  ],
  "_source": ["resMsg"]
}