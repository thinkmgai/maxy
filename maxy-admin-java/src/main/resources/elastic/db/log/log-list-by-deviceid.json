{
  "size": 1000,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },

        #[isDeviceModel]
        {
          "term": {
            "deviceModel": #{deviceModel}
          }
        },
        #[/isDeviceModel]

        #[isDeviceId]
        {
          "term": {
            "deviceId": #{deviceId}
          }
        },
        #[/isDeviceId]

        #[isOsType]
        {
          "term": {
            "osType": #{osType}
      }
        },
        #[/isOsType]
        {
          "range": {
            "logTm": {
              "from": #{from},
              #[isTo]
              "to": #{to},
              #[/isTo]
              "include_lower": true,
              "include_upper": true,
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  },
  "sort": [
    {
      "logTm": {
        "order": "desc"
      }
    }
  ]
}