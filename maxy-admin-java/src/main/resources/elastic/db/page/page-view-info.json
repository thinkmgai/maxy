{
  "aggs": {
    "res": {
      "terms": {
        "field": "reqUrl.raw",
        "order": {
          "_count": "desc"
        },
        "size": 9999
      },
      "aggs": {
        "avgIntervaltime": {
          "avg": {
            "field": "intervaltime"
          }
        },
        "viewer": {
          "cardinality": {
            "field": "deviceId.raw"
          }
        }
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "pageStartTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  }
}