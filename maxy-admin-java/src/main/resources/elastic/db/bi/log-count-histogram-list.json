{
  "aggs": {
    "res": {
      "date_histogram": {
        "field": "logTm",
        "interval": "1d",
        "time_zone": "Asia/Seoul"
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "logTm": {
              "from": #{from},
              "to": #{to},
              "include_lower": true,
              "include_upper": true,
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  }
}