{
  "aggs": {
    "res": {
      "sum": {
        "field": "errorCount"
      }
    }
  },
  "track_total_hits": true,
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "pageStartTm": {
              "from": #{fromDt},
              "to": #{toDt},
              "include_lower": true,
              "include_upper": true,
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  }
}