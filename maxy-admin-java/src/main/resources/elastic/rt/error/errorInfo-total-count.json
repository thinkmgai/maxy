{
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        {
          "range": {
            "logTm": {
              "gte": #{fromTimestamp},
                "lte": #{toTimestamp}
            }
          }
        }
      ]
    }
  }
}