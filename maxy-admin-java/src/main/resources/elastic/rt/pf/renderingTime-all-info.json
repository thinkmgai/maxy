{
  "aggs": {
    "date": {
      "date_histogram": {
        "field": "logDate",
        "interval": "1d",
        "time_zone": "Asia/Seoul"
      },
      "aggs": {
        "osType": {
          "terms": {
            "field": "osType",
            "size": 10
          },
          "aggs": {
            "maxTime": {
              "max": {
                "field": "loadingTime"
              }
            },
            "med(time)": {
              "percentiles": {
                "field": "loadingTime",
                "percents": [50]
              }
            },
            "min(time)": {
              "min": {
                "field": "loadingTime"
              }
            }
          }
        }
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        {
          "range": {
            "logDate": {
              "gte": #{fromTimestamp},
              "lte": #{toTimestamp}
            }
          }
        }
      ]
    }
  }
}