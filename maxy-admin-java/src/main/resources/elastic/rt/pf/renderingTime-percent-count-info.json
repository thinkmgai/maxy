{
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        {
          "term": {
            "osType": #{timeOsType}
          }
        },
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        {
          "range": {
            "logDate": {
              "gte": #{fromTimestamp},
              "lte": #{toTimestamp}
            }
          }
        },
        {
          "range": {
            #{timeType} : {
              "gte": #{time}
            }
          }
        }
      ]
    }
  }
}