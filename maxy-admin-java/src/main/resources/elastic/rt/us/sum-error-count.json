{
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        }
        #[isAppVer]
        ,{
          "term": {
            "appVer": #{appVer}
          }
        }
        #[/isAppVer]
        #[isDateRange]
      ,{
          "range": {
            "logTm": {
              "from": #{fromTimestamp},
        "to": #{toTimestamp},
        "include_lower": true,
        "include_upper": true,
        "time_zone": "Z"
        }
        }
        }
        #[/isDateRange]
      ]
    }
  }
}