{
  "aggs": {
    "res": {
      "max": {
        "field": "usingTime"
      }
    }
  },
  "size": 0,
  "stored_fields": [
    "*"
  ],
  "script_fields": {},
  "docvalue_fields": [
    {
      "field": "appEndTm",
      "format": "date_time"
    },
    {
      "field": "appStartTm",
      "format": "date_time"
    }
  ],
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        {
          "range": {
            "appStartTm": {
              "from": #{fromTimestamp},
              "to": #{toTimestamp},
              "include_lower": true,
              "include_upper": true,
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  }
}