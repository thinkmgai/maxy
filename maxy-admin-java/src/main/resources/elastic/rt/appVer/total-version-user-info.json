{
  "aggs": {
    "user": {
      "date_histogram": {
        "field": "appStartTm",
        "interval": "1d",
        "time_zone": "Asia/Seoul",
        "format": "yyyy-MM-dd",
        "extended_bounds": {
          "min": #{fromDt},
          "max": #{toDt}
        }
      },
      "aggs": {
        "count": {
          "cardinality": {
            "field": "deviceId.raw"
          }
        }
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "range": {
            "appStartTm": {
              "gte": #{fromTimestamp},
              "lte": #{toTimestamp}
            }
          }
        }
      ]
    }
  }
}