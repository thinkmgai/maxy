{
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        {
          "range": {
            "logTm": {
              "gte": #{searchFromDay},
              "lte": #{searchToDay},
              "format": "epoch_millis"
            }
          }
        },
        {
          "term": {
            "logTypeDnm": "App start"
          }
        }
      ]
    }
  }
}