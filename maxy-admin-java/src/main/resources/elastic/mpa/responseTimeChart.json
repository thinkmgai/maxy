{
  "aggs": {
    "key": {
      "date_histogram": {
        "field": "logTm",
        "fixed_interval": #{interval},
        "extended_bounds": {
          "min": #{from},
          "max": #{to}
        }
      },
      "aggs": {
        "avgTime": {
          "avg": {
            "field": "intervaltime"
          }
        },
        "maxTime": {
          "max": {
            "field": "intervaltime"
          }
        },
        "minTime": {
          "min": {
            "field": "intervaltime"
          }
        }
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "terms": {
            "logType": ["8388612", "524291"]
          }
        },
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        #[searchDeviceId]
        {
          "term": {
            "deviceId.raw": #{searchValue}
          }
        },
        #[/searchDeviceId]
        #[searchUserNm]
        {
          "term": {
            "userNm.raw": #{searchValue}
          }
        },
        #[/searchUserNm]
        #[searchUserId]
        {
          "term": {
            "userId.raw": #{searchValue}
          }
        },
        #[/searchUserId]
        {
          "range": {
            "logTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        },
        {
          "range": {
            "intervaltime": {
              "gte": 0
            }
          }
        }
      ]
    }
  }
}