{
  "query": {
    "bool": {
      "must": [
        {
          "bool" : {
            "must" : [
              {
                "term" : {
                  "packageNm" : {
                    "value" : #{packageNm}
                  }
                }
              },
              {
                "term" : {
                  "serverType" : {
                    "value" : #{serverType}
                  }
                }
              },
              #[isAppVer]
              {
                "term" : {
                  "appVer" : {
                    "value" : #{appVer}
                  }
                }
              },
              #[/isAppVer]
              {
                "term" : {
                  "accessDate": {
                    "value": #{accessDate}
                  }
                }
              },
              {
                "term" : {
                  "activeSt" : {
                    "value" : "Y"
                  }
                }
              },
              {
                "term" : {
                  "vipYn" : {
                    "value" : "N"
                  }
                }
              },
              {
                "bool": {
                  "must" : [
                    {
                      "terms": {
                        "deviceSt": ["N", "R"]
                      }
                    },
                    {
                      "term" : {
                        "appReinstallYn": "N"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  }
}