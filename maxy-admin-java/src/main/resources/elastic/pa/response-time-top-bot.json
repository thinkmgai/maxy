{
  "size": 0,
  "aggs": {
    "res": {
      "percentiles": {
        "field": "intervaltime",
        "percents": [5, 95]
      }
    }
  },
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        {
          "terms": {
            "logType": ["8388612", "524291"]
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        {
          "term": {
            "reqUrl.raw": #{reqUrl}
          }
        },
        #[issimOperatorNm]
        {
          "term": {
            "simOperatorNm": #{simOperatorNm}
          }
        },
        #[/issimOperatorNm]

        #[iscomType]
        {
          "term": {
            "comType": #{comType}
          }
        },
        #[/iscomType]
        #[isdeviceModel]
        {
          "term": {
            "deviceModel": #{deviceModel}
          }
        },
        #[/isdeviceModel]
        {
          "range": {
            "logTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        },
        {
          "range": {
            "intervaltime": {
              "gte": 0
            }
          }
        }
      ]
    }
  }
}