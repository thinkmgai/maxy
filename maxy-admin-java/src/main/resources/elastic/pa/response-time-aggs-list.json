{
  "size": 0,
  "aggs": {
    "res": {
      "composite": {
        "sources": [
          {
            "reqUrl": {
              "terms": {
                "field": "reqUrl.raw"
              }
            }
          },
          {
            "deviceModel": {
              "terms": {
                "field": "deviceModel"
              }
            }
          }
        ],
        #[after_key]
        "after": {
          #{after_key}
        },
        #[/after_key]
        "size": #{size}
      },
      "aggs": {
        "max": {
          "max": {
            "field": "intervaltime"
          }
        },
        "min": {
          "min": {
            "field": "intervaltime"
          }
        },
        "avg": {
          "avg": {
            "field": "intervaltime"
          }
        },
        "sorted_buckets": {
          "bucket_sort": {
            "sort": [
              {
                "max": {
                  "order": "desc"
                }
              }
            ]
          }
        }
      }
    }
  },
  "query": {
    "bool": {
      "filter": [
        {
          "terms": {
            "logType": ["8388612", "524291"]
          }
        },
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        #[isDeviceModel]
        {
          "term": {
            "deviceModel": #{deviceModel}
          }
        },
        #[/isDeviceModel]
        #[searchDeviceId]
        {
          "term": {
            "deviceId.raw": #{searchValue}
          }
        },
        #[/searchDeviceId]
        #[searchUserId]
        {
          "term": {
            "userId.raw": #{searchValue}
          }
        },
        #[/searchUserId]
        #[searchUserNm]
        {
          "term": {
            "userNm.raw": #{searchValue}
          }
        },
        #[/searchUserNm]
        {
          "range": {
            "logTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        },
        {
          "range": {
            "intervaltime": {
              "gte": 0
            }
          }
        }
      ]
    }
  }
}