{
  "size": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isOsType]
        {
          "term": {
            "osType": #{osType}
          }
        },
        #[/isOsType]
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        #[searchDeviceId]
            {
              "term":{
                "deviceId.raw" : {
                  "value" : #{searchValue}
                }
              }
            },
      #[/searchDeviceId]
      #[searchUserNm]
            {
              "term":{
                "userNm.raw" : #{searchValue}
            }
          },
      #[/searchUserNm]
      #[searchUserId]
          {
            "term":{
              "userId.raw" : #{searchValue}
            }
          },
      #[/searchUserId]
        #[searchReqUrl]
        {
          "wildcard": {
            "reqUrl.raw": {
              "value": #{searchValue}
            }
          }
        },
        #[/searchReqUrl]
        {
          "range": {
            "pageStartTm": {
              "gte": #{from},
              "lte": #{to},
              "time_zone": "Z"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "res": {
      "terms": {
        "field": "reqUrl.raw",
        "size": 10000
      },
      "aggs": {
        "viewer": {
          "cardinality": {
            "field": "deviceId.raw"
          }
        },
        "avgStayTime": {
          "avg": {
            "field": "intervaltime"
          }
        },
        "avgLoadingTime": {
          "avg": {
            "field": "loadingTime"
          }
        }
      }
    }
  },
  "sort" : [
    {
      "_id" : {
      "order" : "desc",
      "missing" : "_last",
      "unmapped_type" : "long"
      }
    }
  ]
  #[isMoreData]
  , "search_after":[
    #{id}
  ]
  #[/isMoreData]
}