{
  "aggs": {
    "avgMem": {
      "avg": {
        "field": "memUsage"
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "packageNm": #{packageNm}
          }
        },
        {
          "term": {
            "serverType": #{serverType}
          }
        },
        #[isAppVer]
        {
          "term": {
            "appVer": #{appVer}
          }
        },
        #[/isAppVer]
        {
          "term": {
            "deviceModel": #{deviceModel}
          }
        }
      ]
    }
  }
}